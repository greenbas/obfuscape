; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Game",
"language": "GDScript",
"path": "res://src/Games/Game.gd"
}, {
"base": "Node",
"class": "GameData",
"language": "GDScript",
"path": "res://src/GameData.gd"
}, {
"base": "Node",
"class": "QueryGenerator",
"language": "GDScript",
"path": "res://src/Elements/Custom_Elements/QueryGeneratorTemplate.gd"
}, {
"base": "Button",
"class": "SceneChangingButton",
"language": "GDScript",
"path": "res://src/Elements/Custom_Elements/PlayButton.tres.gd"
}, {
"base": "Button",
"class": "VisibilitySwitcherButton",
"language": "GDScript",
"path": "res://src/Elements/Custom_Elements/VisibilitySwitcherButton.gd"
} ]
_global_script_class_icons={
"Game": "",
"GameData": "",
"QueryGenerator": "",
"SceneChangingButton": "",
"VisibilitySwitcherButton": ""
}

[application]

config/name="Obfuscape"
run/main_scene="res://src/globals.tscn"
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

globals="*res://src/globals.tscn"

[debug]

gdscript/warnings/unsafe_property_access=true

[display]

window/size/width=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
